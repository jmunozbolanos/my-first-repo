values = edgecolors_index
tx = np.linspace(x.min(), x.max(), 100)
ty = np.linspace(y.min(), y.max(), 100)
X1, Y1 = np.meshgrid(tx, ty)
rbf = Rbf(x, y, values, function = 'linear', epsilon = 1, smooth = 3)

Z1 = rbf(X1, Y1)

plt.pcolor(X1, Y1, Z1, cmap= cmap1, shading = 'auto', alpha = 0.3, antialiased = True)

plt.scatter(x=x, y=y, s=sizes**2/4, marker='o',
            facecolors=array(facecolors)[facecolors_index], edgecolors=array(edgecolors)[edgecolors_index],
            linewidths=linewidths)
plt.xlabel('PC1')
plt.ylabel('PC2')
plt.tight_layout()


plt.show()